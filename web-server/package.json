{
    "name": "wiki",
    "type": "module",
    "version": "0.5.0",
    "scripts": {
        "dev": "dotenv -e ../.env -- astro dev",
        "start": "dotenv -e ../.env -- astro dev",
        "build": "dotenv -e ../.env -- astro check && dotenv -e ../.env -- astro build",
        "preview": "dotenv -e ../.env -- astro preview",
        "astro": "npm run gen-schema && dotenv -e ../.env --  astro",
        "gen-schema:rust": "cd ../wiki-stats-rs/ && ./target/debug/cli debug gen-stats-json-schema && mv stats-schema.json ../web-server/",
        "gen-schema:ts": "cd ../web-server && npx json-refs resolve stats-schema.json | npx json-schema-to-zod | npx prettier --parser typescript > src/schemas/stats-schema.ts",
        "gen-schema": "npm run gen-schema:rust && npm run gen-schema:ts"
    },
    "dependencies": {
        "@astrojs/check": "^0.9.4",
        "@astrojs/node": "^9.4.4",
        "@astrojs/sitemap": "^3.6.0",
        "@astrojs/solid-js": "^5.1.1",
        "@astrojs/tailwind": "^6.0.2",
        "@solid-primitives/refs": "^1.0.8",
        "@types/better-sqlite3": "^7.6.9",
        "astro": "^5.14.0",
        "better-sqlite3": "^12.4.1",
        "chart.js": "^4.4.2",
        "chartjs-plugin-datalabels": "^2.2.0",
        "clsx": "^2.1.0",
        "dotenv": "^16.4.5",
        "drizzle-orm": "^0.30.1",
        "flowbite": "^2.5.1",
        "fuse.js": "^7.0.0",
        "graphology": "^0.25.4",
        "graphology-generators": "^0.11.2",
        "graphology-graphml": "^0.5.2",
        "graphology-layout": "^0.6.1",
        "graphology-layout-force": "^0.2.4",
        "graphology-layout-forceatlas2": "^0.10.1",
        "graphology-layout-noverlap": "^0.4.2",
        "graphology-types": "^0.24.7",
        "iwanthue": "^2.0.0",
        "node-html-parser": "^6.1.13",
        "pretty-ms": "^9.0.0",
        "redis": "^4.7.0",
        "seedrandom": "^3.0.5",
        "sigma": "^3.0.0-beta.17",
        "solid-chartjs": "^1.3.9",
        "solid-js": "^1.8.15",
        "sql.js": "^1.10.2",
        "tailwind-hamburgers": "^1.3.5",
        "tailwind-merge": "^2.2.2",
        "tailwindcss": "^3.4.1",
        "utility-types": "^3.11.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@iconify-json/material-symbols": "^1.1.78",
        "@types/node": "^20.11.25",
        "@types/seedrandom": "^3.0.8",
        "@types/uuid": "^9.0.8",
        "astro-icon": "^1.1.0",
        "dotenv-cli": "^10.0.0",
        "drizzle-kit": "^0.31.4",
        "json-refs": "^3.0.15",
        "json-schema-to-zod": "^2.6.1",
        "postcss-import": "^16.1.0",
        "prettier": "^3.6.2",
        "prettier-plugin-astro": "^0.14.0",
        "prettier-plugin-astro-organize-imports": "^0.4.8",
        "prettier-plugin-organize-imports": "^4.3.0",
        "sass-embedded": "^1.78.0",
        "ts-node": "^10.9.2",
